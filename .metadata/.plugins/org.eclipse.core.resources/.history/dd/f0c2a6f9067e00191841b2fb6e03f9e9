package LAmbdatrainingpackage;

@FunctionalInterface
interface Calculator{
	public void add (int a, int b);
}

class Calc{
		public static void addSomthing(int n1, int n2) {
			System.out.println(n1 + " and "+n2+" adds to give: "+(n1+n2));
		}
}

public class mathexpressions{
	public static void main(String args[]) {
		Calc.addSomthing(10,20);
	}
}